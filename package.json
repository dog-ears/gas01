{
  "name": "gas01",
  "version": "1.0.0",
  "description": "Google Apps Script learning project",
  "main": "index.js",
  "scripts": {
    "build": "shx rm -rf build && tsc && shx cp src/appsscript.json build/appsscript.json",
    "push": "npm run build && clasp push",
    "push:prod": "shx cp .clasp-prod.json .clasp.json && npm run push",
    "push:staging": "shx cp .clasp-staging.json .clasp.json && npm run push",
    "open": "clasp open-script",
    "open:prod": "shx cp .clasp-prod.json .clasp.json && clasp open-script",
    "open:staging": "shx cp .clasp-staging.json .clasp.json && clasp open-script",
    "typecheck": "tsc --noEmit",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@google/clasp": "^3.1.3",
    "@types/google-apps-script": "^2.0.8",
    "@types/node": "^24.10.1",
    "shx": "^0.4.0",
    "typescript": "^5.9.3"
  }
}
